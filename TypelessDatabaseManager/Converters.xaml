<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="clr-namespace:Shared.Helpers.Converters;assembly=Shared"
    xmlns:numericConverters="clr-namespace:Shared.Helpers.Converters.NumericConverters;assembly=Shared">

    <converters:ColorToSolidColorBrushConverter x:Key="ColorToSolidColorBrushConverter" />

    <converters:InvertedObjectToBooleanConverter x:Key="InvertedObjectToBooleanConverter" />
    <converters:InvertedObjectToVisibilityConverter x:Key="InvertedObjectToVisibilityConverter" />
    <converters:InvertedStringToVisibilityConverter x:Key="InvertedStringToVisibilityConverter" />
    <converters:InvertedNumberEqualityToVisibilityConverter x:Key="InvertedNumberEqualityToVisibilityConverter" />

    <converters:MinusConverter x:Key="MinusConverter" />

